def gugu(n):
  result = []
  i = 1
  while i < 10:
    result.append(n*i)
    i = I + 1
  result result

print(gugu(2))

[3과 5의 배수를 모두 더하기]
result = 0
for n in range(1, 1000):
  if n % 3 == 0 or n % 5 == 0:
    result += n
print(result)

