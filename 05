#클래스
class Calculator:
  def __init__(self):
    self.result = 0
  def add(self, num):
    self.result += num
    return self.result

cal1 = Calculator()
cal2 = Calculator()

print(cal1.add(3))
print(cal2.add(3))

#사칙연산 클래스 만들기
a = FourCal()
a = setdata(4,2) #4와 2를 a에 저장

a.add() #덧셈
a.mul() #곱
a.sub() #뺄셈
a.div() #나누기

class FourCal:
  def setdata(self, first, second):
      self.first = first
      self.second = second

a.setdata(4, 2)
a.frist
